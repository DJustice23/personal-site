{
  "name": "personal-site",
  "version": "4.0.0",
  "homepage": "https://DJustice23.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DJustice23/personal-site.git"
  },
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "build": "rimraf ./build && react-scripts build",
    "predeploy": "npm run build && react-snap --no-sandbox",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "lint": "eslint $(git ls-files '*.js')",
    "test": "react-scripts test --watchAll=false",
    "analyze": "npm run build && source-map-explorer build/static/js/*.chunk.js",
    "format": "prettier --write .",
    "fix": "eslint . --ext .js,.jsx --fix && prettier --write ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "dayjs": "^1.11.13",
    "gh-pages": "^6.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-burger-menu": "^3.1.0",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.28.1",
    "react-scripts": "^5.0.1",
    "react-snap": "^1.23.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-react": "^7.26.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "prettier": "^3.4.2",
    "puppeteer": "^24.1.0",
    "sass": "^1.83.4",
    "source-map-explorer": "^2.5.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
